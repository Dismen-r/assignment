package Assignment2;

public class Attence_date {
int day,month,year;

public Attence_date() {
	super();
}

public int getDay() {
	return day;
}

public void setDay(int day) {
	this.day = day;
}

public int getMonth() {
	return month;
}

public void setMonth(int month) {
	this.month = month;
}

public int getYear() {
	return year;
}

public void setYear(int year) {
	this.year = year;
}

public Attence_date(int day, int month, int year) {
	super();
	this.day = day;
	this.month = month;
	this.year = year;
}

}
